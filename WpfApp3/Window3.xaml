<Window x:Class="WpfApp3.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="WoodBox VM" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Left" Height="419" VerticalAlignment="Top" Width="792" Source="xp_desktop.jpg" Stretch="Fill"/>
        <Image HorizontalAlignment="Left" Height="409" VerticalAlignment="Top" Width="792" Source="ie6.png" Stretch="Fill"/>
        <WebBrowser HorizontalAlignment="Left" Height="309" Margin="0,79,0,0" VerticalAlignment="Top" Width="792" Source="https://google.com" Cursor="Arrow" IsEnabled="False"/>
        <Button Content="X" HorizontalAlignment="Left" Margin="769,0,0,0" VerticalAlignment="Top" Width="23" Click="Button_Click" Height="19" RenderTransformOrigin="0.5,0.5">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0.754"/>
                    <RotateTransform Angle="0.335"/>
                    <TranslateTransform X="0.112"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>

    </Grid>
</Window>
